{
	"description": "(Microsoft) Teams for Web Improved",
	"manifest_version": 2,
	"name": "teams-wim",
	"version": "0.0.1",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"notifications",
		"https://*.teams.microsoft.com/*"
	],
	"background": {
		"scripts": ["notifications.js"]
	},
	"applications": {
		"gecko": {
			"id": "teams-wim@maxbruckner.de",
			"strict_min_version": "60.0"
		}
	}
}
